{
  "name": "react-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lesson": "node ./goto-lesson.js",
    "lint": "concurrently -p -n a,s \"npm:lint:app\" \"npm:lint:server\"",
    "lint:watch": "concurrently -p -n a,s \"npm:lint:app:watch\" \"npm:lint:server:watch\"",
    "lint:app": "eslint ./src",
    "lint:app:watch": "esw --watch --color ./src",
    "lint:server": "eslint ./server",
    "lint:server:watch": "esw --watch --color ./server",
    "build:dev:server": "babel ./server -d dist",
    "build:dev:server:watch": "babel ./server -d dist --watch",
    "build:dev:app": "webpack --config=webpack.js",
    "build:dev:app:watch": "npm run build:dev:app -- --watch",
    "build:dev": "concurrently -p -n \"   app\",server,\"  lint\"  \"npm:build:dev:app\" \"npm:build:dev:server\" \"npm:lint\"",
    "build:dev:watch": "concurrently -p -n \"   app\",server  \"npm:build:dev:app:watch\" \"npm:build:dev:server:watch\"",
    "start:watch": "nodemon --delay 2 ./dist/index.js",
    "start:dev": "concurrently -p -n \"build\",\"  run\" \"npm:build:dev:watch\" \"npm:start:watch\""
  },
  "author": "",
  "license": "ISC",
  "lesson": 1,
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.0",
    "jest": "^24.8.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.21.1",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "concurrently": "^4.1.1",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  }
}
